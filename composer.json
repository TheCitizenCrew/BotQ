{
    "name": "Bot Queue",
    "description": "A Bot actions queue",
    "keywords": ["bot","queue"],
    "license": "GPL v3",
    "type": "project",
    "require": {
				"laravel/lumen-framework": "5.0.*",
				"vlucas/phpdotenv": "~1.0",
				"laravelcollective/html": "5.0.*",
				"aziz/smartmodel": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
