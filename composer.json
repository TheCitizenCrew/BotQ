{
    "name": "Bot Queue",
    "description": "A Bot actions queue",
    "keywords": ["bot","queue"],
    "license": "GPL v3",
    "type": "project",
    "require": {
				"laravel/lumen-framework": "5.0.*",
				"vlucas/phpdotenv": "~1.0",
				"esensi/model": "0.5.*",
				"components/jquery": "2.1.*",
				"happyworm/jplayer": "2.9.*",
				"twitter/bootstrap": "3.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
	"component-dir": "public/js",
	"component-baseurl": "/js"
    }
}
